### Ambiente HomeLab

Host srm
   HostName srm.alejakun.io
   Port 2222
   User alex

Host nasty
   HostName nasty.alejakun.io
   Port 2222
   User alex

Host rbp
   HostName rbp.alejakun.io
   Port 22
   User alex
   IdentityFile ~/.ssh/homelab/id_rsa

Host oci
   HostName homelab.alejakun.io
   Port 22
   # User opc
   User ubuntu
   IdentityFile ~/.ssh/homelab/oci-ssh-key.key

Host *
   AddKeysToAgent yes
   UseKeychain yes
   IdentityFile ~/.ssh/id_ed25519
